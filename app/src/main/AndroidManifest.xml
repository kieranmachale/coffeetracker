<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.myassignment">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyAssignment">
        <activity android:name=".DetailsActivity"
            android:parentActivityName=".CafeListActivity"
            android:label="Details"/>
        <activity android:name=".FavouritesActivity"
            android:parentActivityName="ProfileActivity"/>
        <activity android:name=".CafeListActivity"
            android:parentActivityName=".MenuActivity"/>
        <activity android:name=".ProfileInfoActivity" />
        <activity android:name=".LanguageActivity" />
        <activity android:name=".WebSearchActivity"
            android:parentActivityName="MenuActivity"/>
        <activity android:name=".ProfileActivity"
            android:parentActivityName="MenuActivity"/>
        <activity android:name=".MenuActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>